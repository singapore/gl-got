{
  "name": "gl-got",
  "version": "8.0.0",
  "description": "Convenience wrapper for `got` to interact with the GitLab API",
  "license": "MIT",
  "repository": "singapore/gl-got",
  "author": {
    "name": "Rhys Arkins",
    "email": "rhys@keylocation.sg"
  },
